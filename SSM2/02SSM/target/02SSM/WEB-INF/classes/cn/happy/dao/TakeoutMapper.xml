<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.happy.dao.TakeoutMapper">
    <insert id="addTakeout" parameterType="cn.happy.model.Takeout">
        insert into takeout(quantity,outdate,handler,productid) VALUES(#{quantity},#{outdate},#{handler},#{productid})
    </insert>

</mapper>